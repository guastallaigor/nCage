{
  "name": "bubuCage",
  "short_name": "bubuCage",
  "description": "Replaces all images on all pages to some of my friends whatsapp stickers",
  "version": "0.1",
  "author": "Igor Guastalla",
  "manifest_version": 2,
  "icons": {
    "72": "images/icons/icon-72x72.png",
    "96": "images/icons/icon-96x96.png",
    "128": "images/icons/icon-128x128.png",
    "144": "images/icons/icon-144x144.png",
    "152": "images/icons/icon-152x152.png",
    "384": "images/icons/icon-384x384.png",
    "512": "images/icons/icon-512x512.png"
  },
  "content_scripts": [
    {
      "js": ["bubucage.js"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "related_applications": [{
    "platform": "play",
    "url": "https://chrome.google.com/webstore/detail/ncage/hnbmfljfohghaepamnfokgggaejlmfol?hl=en"
  }]
}
